{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "noEmit": true,
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "app/**/*",
    "app/index.tsx"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}